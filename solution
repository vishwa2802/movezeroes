public class MoveZeroes {
    public static void moveZeroes(int[] nums){
        int index = 0;
        // move all non zeroes element to the front side of array
        for(int i = 0;i< nums.length;i++){
            if(nums[i] !=0){
                nums[index] = nums[i];
                index++;
            }
        }
        // fill the non zeroes element
        while(index < nums.length){
            nums[index] = 0;
            index++;
        }
    }
    public static void main(String[] args){
        int[] nums ={0,1,0,3,12};
moveZeroes(nums);
// print the array
for(int : nums){
    System.out.print(num + " ");
}
    }

}